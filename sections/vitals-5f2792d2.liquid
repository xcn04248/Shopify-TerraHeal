		window.vtlsBreakpoints = {
			mobile: {
				minWidth: 0,
				maxWidth: 479,
			},
			tablet: {
				minWidth: 480,
				maxWidth: 991,
			},
			desktop: {
				minWidth: 992,
				maxWidth: 10000,
			},
		};
